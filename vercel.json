{
  "version": 2,
   "builds": [
      {
          "src": "./index.py",
          "use": "@vercel/python"
      }, 
     {
            "src": "./static/images",
            "use": "@vercel/static?images"
        }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ]
}

